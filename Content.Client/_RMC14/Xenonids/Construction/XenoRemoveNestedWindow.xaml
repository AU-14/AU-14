<controls:XenoRemoveNestedWindow
    xmlns="https://spacestation14.io"
    xmlns:controls="clr-namespace:Content.Client._RMC14.Xenonids.Construction"
    Title="Confirmation"
    MinSize="300 200">
    <BoxContainer Orientation="Vertical">
        <Label Text ="The target is still alive and kicking! Are we sure we want to remove them from the nest?"/>
        <Button Name="ConfirmButton" Access="Public" MinHeight="35" Text="Yes"/>
        <Button Name="DenyButton" Access="Public" MinHeight="35" Text="No"/>
    </BoxContainer>
</controls:XenoRemoveNestedWindow>
